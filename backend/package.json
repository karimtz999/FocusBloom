    "db:reset": "prisma migrate reset",
    "db:studio": "prisma studio",
    "admin": "tsx src/scripts/adminManagerPG.ts",
    "admin:demo": "node src/scripts/postgresDemo.js",
    "admin:old": "node src/scripts/adminManager.js",
    "setup-admin": "tsx src/scripts/createSuperAdminPG.ts",
    "setup-admin:old": "node src/scripts/createSuperAdmin.js",
    "createTestUser": "tsx src/scripts/createTestUser.ts"
