    "@types/node": "^20.8.0",
    "@types/supertest": "^6.0.3",
    "jest": "^30.0.3",
    "mongodb-memory-server": "^10.1.4",
    "nodemon": "^3.0.1",
    "rimraf": "^5.0.5",
    "supertest": "^7.1.1",
    "ts-jest": "^29.4.0",
    "ts-node": "^10.9.2",
    "tsx": "^4.20.3",
    "typescript": "^5.2.2"
  }
